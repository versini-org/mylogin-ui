{
	"name": "@mylogin/client",
	"version": "2.1.2",
	"license": "MIT",
	"author": "Arno Versini",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"dev": "rsbuild dev",
		"build:bundlesize": "rsbuild build -c \"../../configuration/rsbuild-bundlesize.config.js\" ",
		"build:js": "rsbuild build",
		"build": "npm-run-all --serial clean build:js",
		"lint": "biome lint src",
		"preview": "rsbuild preview",
		"start": "static-server dist --port 5173",
		"stats:pr": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o tmp/stats.json --silent",
		"stats:release": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o stats/stats.json --silent",
		"stats:report": "bundlesize -c bundlesize.config.js --type report -o tmp/pr-stats.md --silent",
		"stats": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\"",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@versini/ui-styles": "1.10.2"
	},
	"dependencies": {
		"@versini/auth-provider": "7.3.2",
		"@versini/sassysaint": "5.1.2",
		"@versini/ui-anchor": "1.1.8",
		"@versini/ui-button": "1.1.8",
		"@versini/ui-card": "1.0.7",
		"@versini/ui-footer": "1.0.7",
		"@versini/ui-header": "1.0.7",
		"@versini/ui-hooks": "4.2.1",
		"@versini/ui-icons": "1.13.0",
		"@versini/ui-main": "1.0.7",
		"@versini/ui-menu": "1.0.8",
		"@versini/ui-panel": "1.0.8",
		"@versini/ui-system": "1.4.18",
		"@versini/ui-table": "1.0.8",
		"@versini/ui-textarea": "1.0.6",
		"@versini/ui-textinput": "1.1.3",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.26.2",
		"uuid": "10.0.0"
	}
}
