{
	"name": "@mylogin/client",
	"version": "2.8.0",
	"license": "MIT",
	"author": "Arno Versini",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist",
		"dev": "rsbuild dev",
		"build:bundlesize": "rsbuild build -c \"../../configuration/rsbuild-bundlesize.config.js\" ",
		"build:js": "rsbuild build",
		"build": "npm-run-all --serial clean build:js",
		"lint": "biome lint src",
		"preview": "rsbuild preview",
		"start": "static-server dist --port 5173",
		"stats:pr": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o tmp/stats.json --silent",
		"stats:release": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\" -o stats/stats.json --silent",
		"stats:report": "bundlesize -c bundlesize.config.js --type report -o tmp/pr-stats.md --silent",
		"stats": "bundlesize -c bundlesize.config.js -p \"$npm_package_version\"",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:coverage:ui": "vitest --coverage --ui",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@versini/ui-styles": "2.0.0"
	},
	"dependencies": {
		"@versini/auth-provider": "7.4.0",
		"@versini/sassysaint": "5.4.10",
		"@versini/ui-anchor": "1.2.12",
		"@versini/ui-button": "3.0.0",
		"@versini/ui-card": "2.0.3",
		"@versini/ui-footer": "2.0.3",
		"@versini/ui-header": "2.0.3",
		"@versini/ui-hooks": "4.3.0",
		"@versini/ui-icons": "3.0.0",
		"@versini/ui-main": "2.0.3",
		"@versini/ui-menu": "2.0.4",
		"@versini/ui-panel": "1.2.9",
		"@versini/ui-system": "2.0.4",
		"@versini/ui-table": "2.0.4",
		"@versini/ui-textarea": "2.0.3",
		"@versini/ui-textinput": "2.0.3",
		"@versini/ui-togglegroup": "2.0.3",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-router-dom": "7.1.1",
		"uuid": "11.0.3"
	}
}
